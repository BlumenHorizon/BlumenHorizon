[Unit]
Description=Gunicorn instance to serve BlumenHorizon application in Cannes
After=network.target

[Service]
User=root
Group=root
WorkingDirectory=/var/www/blumenhorizon_cannes
ExecStart=/var/www/blumenhorizon_cannes/.venv/bin/gunicorn -c gunicorn.conf.py

[Install]
WantedBy=multi-user.target

